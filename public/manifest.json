  "shortcuts": [
    {
      "name": "Latest News",
      "short_name": "Latest",
      "description": "View the latest cryptocurrency news",
      "url": "/?category=latest",
      "icons": [{ "src": "/icons/shortcut-latest.png", "sizes": "96x96" }]
    },
    {
      "name": "Breaking News",
      "short_name": "Breaking",
